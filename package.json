  "description": "A Node.js Minecraft AFK bot using Mineflayer for Render",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build:frontend": "esbuild public/src/index.tsx --bundle --outfile=public/bundle.js --jsx=automatic",
    "postinstall": "npm run build:frontend"
