{
  "name": "minecraft-afk-bot",
  "version": "1.0.0",
  "description": "A Node.js Minecraft AFK bot using Mineflayer for Render",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node index.js",
    "build": "esbuild public/index.tsx --bundle --outfile=public/bundle.js --loader:.tsx=tsx --resolve-extensions=.tsx,.ts,.js"
  },
  "dependencies": {
    "canvas": "^3.2.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "lucide-react": "^0.562.0",
    "minecraft-data": "^3.102.2",
    "mineflayer": "^4.33.0",
    "mineflayer-auto-auth": "^2.0.0",
    "mineflayer-web-inventory": "^1.8.5",
    "prismarine-viewer": "^1.33.0",
    "proxy-agent": "^6.5.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "socks": "^2.8.7",
    "vec3": "^0.1.10"
  },
  "engines": {
    "node": "^22.0.0"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
